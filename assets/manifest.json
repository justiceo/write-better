{
    "name": "Write Better",
    "version": "0.1.1",
    "description": "English grammar suggestions for better writing",
    "manifest_version": 2,
    "key": "",
    "permissions": [
        "https://docs.google.com/*",
        "storage"
    ],
    "page_action": {
        "default_icon": {
            "16": "images/quill-orange16.png",
            "24": "images/quill-orange24.png",
            "32": "images/quill-orange32.png"
        },
        "default_title": "Write Better",
        "default_popup": ""
    },
    "content_scripts": [
        {
            "all_frames": true,
            "matches": [
                "https://docs.google.com/*"
            ],
            "include_globs": [
                "https://docs.google.com/document/d/*"
            ],
            "js": [
                "content-script.js"
            ],
            "css": [
                "highlight.css"
            ]
        }
    ],
    "icons": {
        "16": "images/quill-orange16.png",
        "32": "images/quill-orange32.png",
        "48": "images/quill-orange48.png",
        "128": "images/quill-orange128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}